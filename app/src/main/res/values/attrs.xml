<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="setLabel" format="string"/>
        <attr name="setHint" format="string"/>
        <attr name="setText" format="string"/>
        <attr name="getText" format="string"/>
        <attr name="isEnabled" format="boolean"/>
        <attr name="android:inputType"/>
        <attr name="android:imeOptions"/>
    </declare-styleable>

        <declare-styleable name="LoopViewPager">
            <attr name="isInfinite" format="boolean" />
            <attr name="autoScroll" format="boolean" />
            <attr name="wrap_content" format="boolean" />
            <attr name="scrollInterval" format="integer"/>
            <attr name="viewpagerAspectRatio" format="float"/>
            <attr name="itemAspectRatio" format="float"/>
        </declare-styleable>

        <declare-styleable name="CustomTabIndicator">
            <attr name="indicator_spacing" format="dimension"/>
        </declare-styleable>

</resources>